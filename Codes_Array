//Array kth min and max-----------------------------------------------------------
#include<stdio.h>
int main()
{
    int n,i,j,k;
    scanf("%d",&n);
    int A[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&A[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<n-i-1;j++)
        {
            if(A[j]>A[j+1])
            {
                A[j]=A[j]^A[j+1];
                A[j+1]=A[j]^A[j+1];
                A[j]=A[j]^A[j+1];
            }
        }
    }
    printf("ENTER K :");
    scanf("%d",&k);
    printf("%dth MAXIMUM : %d\n",k,A[n-k]);
    printf("%dth MINIMUM : %d\n",k,A[k-1]);
    return 0;
}
